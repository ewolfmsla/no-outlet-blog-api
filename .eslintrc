{
  "extends": "airbnb-base",
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "quotes": [2, "single", { "avoidEscape": true }],
        "semi": ["error", "never"],
        "no-underscore-dangle": ["error", { "allow": ["_id"] }]
      }
    }
  ],
  "parser": "@babel/eslint-parser"
}
